@model Factory.Models.Engineer

<div id="edit-button-div" class="bordered">
<button id="edit-button" class="btn btn-dark btn-sm" type="button">Edit engineer</button>
</div>
<div class="initially-hidden bordered" id="editor-div">
  <form action="/Engineers/Edit/" method="post">
    @Html.HiddenFor(model => model.EngineerId)
    @Html.LabelFor(model => model.FirstName, "First Name:")
    @Html.EditorFor(model => model.FirstName)
    @Html.LabelFor(model => model.FirstName, "Last Name:")
    @Html.EditorFor(model => model.LastName)
    <button class="btn btn-dark btn-sm">Save changes</button>
    <button id="cancel-edit-button" class="btn btn-dark btn-sm" type="button">Discard changes</button>
  </form>
</div>